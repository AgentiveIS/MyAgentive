{
  "name": "myagentive",
  "version": "0.4.4",
  "type": "module",
  "description": "MyAgentive, a multi-interface personal AI Agent application (Web, Telegram, CLI)",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "bun --watch server/index.ts",
    "dev:client": "vite --port 5173",
    "start": "bun server/index.ts",
    "build": "vite build && tsc -p tsconfig.server.json",
    "build:binary": "bun build --compile server/index.ts --outfile dist/myagentive",
    "build:binary:linux": "bun build --compile --target=bun-linux-x64 server/index.ts --outfile dist/myagentive-linux",
    "db:migrate": "bun scripts/migrate.ts",
    "setup:skills": "python3 -m venv .venv && .venv/bin/pip install -r requirements-skills.txt",
    "postinstall": "[ -f requirements-skills.txt ] && npm run setup:skills || true"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.28",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "grammy": "^1.31.0",
    "playwright": "^1.57.0",
    "pptxgenjs": "^4.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-use-websocket": "^4.13.0",
    "sharp": "^0.34.5",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.5.0",
    "vite": "^5.4.0"
  }
}
