{
  "name": "myagentive",
  "version": "0.5.11",
  "type": "module",
  "description": "MyAgentive, a multi-interface personal AI Agent application (Web, Telegram, CLI)",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "bun --watch server/index.ts",
    "dev:client": "vite --port 5173",
    "start": "bun server/index.ts",
    "build": "vite build && tsc -p tsconfig.server.json",
    "build:binary": "bun build --compile server/index.ts --outfile dist/myagentive",
    "build:binary:linux": "bun build --compile --target=bun-linux-x64 server/index.ts --outfile dist/myagentive-linux",
    "db:migrate": "bun scripts/migrate.ts",
    "setup:skills": "python3 -m venv .venv && .venv/bin/pip install -r requirements-skills.txt",
    "postinstall": "[ -f requirements-skills.txt ] && npm run setup:skills || true"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.28",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "grammy": "^1.31.0",
    "lucide-react": "^0.562.0",
    "next-themes": "^0.4.6",
    "playwright": "^1.57.0",
    "pptxgenjs": "^4.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "rehype-katex": "^7.0.0",
    "remark-math": "^6.0.0",
    "katex": "^0.16.0",
    "mermaid": "^10.9.0",
    "react-use-websocket": "^4.13.0",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/bun": "^1.1.14",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.0",
    "vite": "^7.3.0",
    "vite-plugin-pwa": "^1.2.0"
  }
}
